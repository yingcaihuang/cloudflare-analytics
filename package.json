  "devDependencies": {
    "@babel/eslint-parser": "^7.28.6",
    "@eslint/js": "^9.39.2",
    "@types/react": "~19.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-native": "^5.0.0",
    "prettier": "^3.8.0",
    "typescript": "~5.9.2"
  },
  "private": true

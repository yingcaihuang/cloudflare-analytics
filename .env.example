# Cloudflare Analytics - Environment Variables Example
# Copy this file to .env and fill in your values
# DO NOT commit .env to version control

# Cloudflare API Endpoints
CLOUDFLARE_API_ENDPOINT=https://api.cloudflare.com/client/v4
CLOUDFLARE_GRAPHQL_ENDPOINT=https://api.cloudflare.com/client/v4/graphql

# Cache Configuration
CACHE_TTL=3600
CACHE_MAX_SIZE=52428800

# App Configuration
APP_ENV=development
DEBUG_MODE=true

# Note: API tokens are stored securely in the app using Expo SecureStore
# You don't need to add them here
